function idSerch(){
	let name = $('input[name=serchName]').val();
	let jumin1 = $('input[name=serchJumin1]').val().trim();
	let jumin2 = $('input[name=serchJumin2]').val().trim();
	
	let serchJumin = jumin1 + jumin2;
	
	if(name.length == 0 || name == ''){
		alert('이름을 입력하세요');
		return false;
	}
	if(serchJumin.length == 0 || serchJumin == ''){
		alert('주민번호를 입력하세요');
		return false;
	}
	
	if(isNaN(jumin1) || isNaN(jumin2)) {
		alert('잘못된 주민번호 입니다.');
		return false
	}
	
	let sum = 0;
	for (let i=0; i<12; i++){
		sum += serchJumin.charAt(i) * (i % 8 + 2);
	}
	
	let result = (11 - sum % 11) % 10;
	
	if(result != serchJumin.charAt(12)) {
		alert('잘못된 주민번호 입니다.');
		return false;
	}
}