function passwordChange() {
	let password1 = $('#password').val();
	let password2 = $('#password2').val();

	if(password2.length == 0) {
		alert('비밀번호를 입력하세요');
		return false;
	}	
	if (password1.trim() != password2.trim()) {
	   alert('비밀번호가 일치하지 않습니다.');
	   return false;
	}
	$('#pwdChkMessage2').html('');
	
	return true;
}